<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kichi.MaestroHP">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Login"
        android:windowSoftInputMode="stateHidden">
        <activity android:name="com.example.kichi.MaestroHP.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.kichi.MaestroHP.RegistroActivity" />
        <activity android:name="com.example.kichi.MaestroHP.MenuActivity" />
        <activity
            android:name="com.example.kichi.MaestroHP.Menu2Activity"
            android:label="@string/title_activity_menu2"
            android:theme="@style/Login"></activity>
    </application>

</manifest>